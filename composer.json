{
    "name": "codica-studio/googlecalendar",
    "description": "A random Codica Studio package.",
    "keywords": [
        ""
    ],
    "homepage": "https://github.com/codica-studio/googlecalendar",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0", 
        "google/apiclient": "^2.7",
        "illuminate/support": "^8.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.0|^1.3.1",
        "orchestra/testbench": "^^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\GoogleCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\GoogleCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\GoogleCalendar\\GoogleCalendarServiceProvider"
            ],
            "aliases": {
                "GoogleCalendar": "Spatie\\GoogleCalendar\\GoogleCalendarFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
